<div class="form-group row">
  <label class="col-form-label col-sm-2" for="Title">Title</label>
  <div class="col-sm-8">
    <input
      readonly
      class="form-control"
      id="Title"
      value="<%= model.Title %>"
    />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="Author">Author</label>
  <div class="col-sm-7">
    <input
      readonly
      class="form-control"
      id="Author"
      value="<%= model.Author %>"
    />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="ISBN">ISBN</label>
  <div class="col-sm-7">
    <input readonly class="form-control" id="ISBN" value="<%= model.ISBN %>" />
  </div>
</div>
